<% @listings.each do |l| %>
<div class="card" style="width: 20rem;">
  <%= image_tag l.image.url(:medium), class:"card-img-top" %>
  <div class="card-block">
    <h4 class="card-title"><%= truncate(l.title, length: 10) %></h4>
    <p class="card-text"><%= l.price %><%= l.description %></p>
    <%= link_to "More Info", l, class: "btn btn-primary" %>
  </div>
</div>
<% end %>
